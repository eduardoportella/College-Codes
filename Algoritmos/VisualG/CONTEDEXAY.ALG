algoritmo "contedexay"
var
a, b, c: real

inicio
      escreval("digite o valor para contar")
      leia(a)
      escreval("qual sera o valor do salto?")
      leia(b)
      escreval("---------------------------")
      c <- 0
      enquanto (c<=a) faca
      escreval(c)
      c <- c+b
      fimenquanto

fimalgoritmo