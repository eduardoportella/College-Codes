algoritmo "parouimparprocedimento"
var
n: inteiro

procedimento parouimpar(v: inteiro)
inicio
   escreval("o valor de V eh", V)
   escreval("o valor de N eh", N)
   se (V%2=0) entao
      escreval("o numero", v, " eh par")
   senao
      escreval("o numero", v, " eh impar")
   fimse
fimprocedimento


inicio
   escreva("digite um numero: ")
   leia(n)
   parouimpar(n)

fimalgoritmo