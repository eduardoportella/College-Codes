algoritmo "conteatex"
var
a, b, c: inteiro

procedimento contagem (a: inteiro)
inicio
      enquanto (b<=a) faca
      escreval(b)
      b <- b+1
      fimenquanto
fimprocedimento


inicio
   escreval("ate que numero voce quer contar?")
   leia(c)
   a <- c
   escreval("--------------------------------")
   contagem(a)

fimalgoritmo